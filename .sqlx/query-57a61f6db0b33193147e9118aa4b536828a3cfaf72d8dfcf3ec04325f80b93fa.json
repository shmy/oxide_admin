  "query": "\n        SELECT id, name, path, size, used, created_at, updated_at\n        FROM _files\n        WHERE ($1::text IS NULL OR name LIKE CONCAT('%', $1, '%'))\n        AND ($2::boolean IS NULL OR used = $2)\n        ORDER BY created_at DESC LIMIT $3 OFFSET $4 \n        ",
